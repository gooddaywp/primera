{
    "name": "gooddaywp/primera",
    "type": "project",
    "description": "A MVC starter theme for WordPress using Laravel's Blade templating engine.",
    "require": {
        "gooddaywp/primera-lib": "@dev"
    },
    "repositories": {
        "dev-package": {
            "type": "path",
            "url": "~/code/primera-lib",
            "options": {
                "symlink": true
            }
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
